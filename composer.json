    "ext-curl" : "*",
    "theseer/fdomdocument" : ">=1.5.0"
  },
  "require-dev" : {
    "codeception/codeception" : "=1.8.5",
    "mockery/mockery" : "dev-master",
    "phpunit/phpunit" : "3.7.*",
    "phpunit/phpunit-selenium" : "=1.3.3",
    "phpunit/php-invoker" : "=1.1.3",
    "phpunit/dbunit" : "=1.3.1",
    "phpunit/phpunit-story" : "=1.0.2",
    "phake/phake" : "1.*",
    "hot/phpunit-runner" : "dev-master"
  },
  "scripts" : {
    "post-install-cmd" : [ "echo [NOTICE] Deleting all .git dirs inside project", "rm -rf .git" ]
  },
  "autoload" : {
    "psr-0" : {
      "" : "src/"
    }
  },
  "config" : {
    "bin-dir" : "bin"
  },
  "suggest" : {
    "ext/xdebug" : "Debugger and profiling"
